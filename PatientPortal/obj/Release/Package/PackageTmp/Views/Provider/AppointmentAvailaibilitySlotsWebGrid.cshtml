@model List<string>

@{
    var grid = new WebGrid(Model, canPage: false, canSort:false, defaultSort: "Name");
}

@if (Model.Count() == 0)
{
    <p>No data found</p>
}
else
{   
   @grid.GetHtml(htmlAttributes: new {id ="grid"}, 
    tableStyle: "table table-striped table-hover",
    headerStyle: "webgrid-header",
    footerStyle: "webgrid-footer",
    alternatingRowStyle: "webgrid-row-style",
    selectedRowStyle: "webgrid-selected-row",
    rowStyle: "webgrid-row-style",
    columns: grid.Columns(
            grid.Column("Select", format: @<text><input id="assignChkBx" name="assignChkBx" type="checkbox" value="@item"/></text>),
            grid.Column("Time", format: @<text>@item</text>)
          )
     )
}
